{
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx-cloud",
            "options": {
                "cacheableOperations": [
                    "build",
                    "test",
                    "lint",
                    "package",
                    "prepare"
                ],
                "accessToken": "ZDQ1NzgyNGEtZTAzMC00MWE2LWIxZTAtOTRmYzhjMzdjYWYyfHJlYWQtd3JpdGU=",
                "skipNxCache": true
            }
        }
    },
    "extends": "nx/presets/npm.json",
    "npmScope": "itmat-broker",
    "affected": {
        "defaultBase": "master"
    },
    "generators": {
        "@nx/react": {
            "application": {
                "style": "css",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "css"
            },
            "library": {
                "style": "css",
                "linter": "eslint"
            }
        }
    },
    "targetDefaults": {
        "test": {
            "inputs": [
                "default",
                "^default",
                "{workspaceRoot}/jest.preset.js"
            ]
        },
        "lint": {
            "inputs": [
                "default",
                "{workspaceRoot}/.eslintrc.json",
                "{workspaceRoot}/.eslintrc.js"
            ]
        },
        "e2e": {
            "inputs": [
                "default",
                "^default"
            ]
        }
    },
    "defaultProject": "itmat-ui-react",
    "$schema": "./node_modules/nx/schemas/nx-schema.json"
}