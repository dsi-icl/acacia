{
    "tasksRunnerOptions": {
        "default": {
            "options": {
                "skipNxCache": true
            }
        }
    },
    "extends": "nx/presets/npm.json",
    "affected": {
        "defaultBase": "master"
    },
    "generators": {
        "@nx/react": {
            "application": {
                "style": "css",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "css"
            },
            "library": {
                "style": "css",
                "linter": "eslint"
            }
        }
    },
    "targetDefaults": {
        "test": {
            "inputs": [
                "default",
                "^default",
                "{workspaceRoot}/jest.preset.js"
            ],
            "cache": true
        },
        "lint": {
            "inputs": [
                "default",
                "{workspaceRoot}/.eslintrc.json",
                "{workspaceRoot}/.eslintrc.js"
            ],
            "cache": true
        },
        "e2e": {
            "inputs": [
                "default",
                "^default"
            ]
        },
        "build": {
            "cache": true
        },
        "package": {
            "cache": true
        },
        "prepare": {
            "cache": true
        }
    },
    "defaultProject": "itmat-ui-react",
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "nxCloudAccessToken": "ZDQ1NzgyNGEtZTAzMC00MWE2LWIxZTAtOTRmYzhjMzdjYWYyfHJlYWQtd3JpdGU="
}