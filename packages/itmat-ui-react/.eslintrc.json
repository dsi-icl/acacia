{
    "extends": [
        "plugin:@nrwl/nx/react",
        "../../.eslintrc.json"
    ],
    "ignorePatterns": [
        "!**/*",
        "node_modules",
        "msrCrypto.js"
    ],
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.tsx",
                "*.js",
                "*.jsx"
            ],
            "rules": {
                "@typescript-eslint/no-implicit-any": "off"
            }
        },
        {
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "rules": {}
        },
        {
            "files": [
                "*.js",
                "*.jsx"
            ],
            "rules": {}
        }
    ]
}