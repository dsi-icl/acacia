{
    "name": "ukb-curator",
    "version": "1.0.0",
    "description": "Curator service for UKB clinical data",
    "author": "Chon Sou",
    "main": "build/index",
    "types": "build/index",
    "files": [
        "build"
    ],
    "scripts": {
        "start": "node ./build/src/index.js",
        "build": "npm run clean && npm run compile",
        "clean": "rimraf -rf ./dist ./build",
        "compile": "tsc -p tsconfig.build.json",
        "test": "npm run compile && jest",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "body-parser": "1.19.0",
        "connect-timeout": "1.9.0",
        "csv-parse": "4.6.1",
        "express": "4.17.1",
        "itmat-commons": "0.0.1",
        "itmat-utils": "1.1.8",
        "mongodb": "3.3.2",
        "saslprep": "1.0.3",
        "node-fetch": "2.6.0"
    },
    "devDependencies": {
        "@types/connect-timeout": "0.0.34",
        "@types/express": "4.17.1",
        "@types/mongodb": "3.3.5",
        "@types/multer": "1.3.10",
        "@types/node-fetch": "2.5.2",
        "@types/node": "12.7.11",
        "eslint-plugin-typescript": "0.14.0",
        "rimraf": "3.0.0",
        "supertest": "4.0.2",
        "typescript-eslint-parser": "21.0.2",
        "typescript": "3.6.3"
    }
}